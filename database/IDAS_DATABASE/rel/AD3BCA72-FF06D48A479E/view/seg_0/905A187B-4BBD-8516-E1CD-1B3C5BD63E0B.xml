<userDefinedSQL><![CDATA[SELECT
    komentar.*,
    uzivatel.jmeno,
    uzivatel.prijmeni
FROM
         komentar
    INNER JOIN uzivatel ON uzivatel.id_uzivatel = komentar.uzivatel_id]]></userDefinedSQL>
