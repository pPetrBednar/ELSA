<userDefinedSQL><![CDATA[SELECT
    uzivatel.id_uzivatel,
    uzivatel.login,
    uzivatel.jmeno,
    uzivatel.prijmeni,
    uzivatel.email,
    uzivatel.telefon,
    uzivatel.adresa,
    uzivatel.image,
    role.opravneni
FROM
         uzivatel
    INNER JOIN role ON role.id_role = uzivatel.role_id]]></userDefinedSQL>
